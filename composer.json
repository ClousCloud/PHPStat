{
    "name": "nurazlib/phpstat",
    "description": "A static analysis tool for PHP, inspired by PHPStan.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "nikic/php-parser": "^4.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "nurazlib\\phpstat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
